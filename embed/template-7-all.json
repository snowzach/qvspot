{
    "settings": {
        "index": {
            "number_of_replicas": 0,
            "number_of_shards": 3,
            "refresh_interval": "15s",
            "mapping.ignore_malformed": true
        }
    },
    "mappings": {
        "dynamic_templates": [
            {
                "attr": {
                    "path_match": "attr.*",
                    "mapping": {
                        "type": "keyword"
                    }
                }
            },
            {
                "attr_num": {
                    "path_match": "attr_num.*",
                    "mapping": {
                        "type": "double"
                    }
                }
            },
            {
                "product.attr": {
                    "path_match": "product.attr.*",
                    "mapping": {
                        "type": "keyword"
                    }
                }
            },
            {
                "product.attr_num": {
                    "path_match": "product.attr_num.*",
                    "mapping": {
                        "type": "double"
                    }
                }
            },
            {
                "star_as_keyword": {
                    "match_mapping_type": "*",
                    "mapping": {
                        "type": "keyword"
                    }
                }
            }
        ],
        "properties": {
            "type": {
                "type": "keyword"
            },
            "id": {
                "type": "keyword"
            },
            "created": {
                "type": "date"
            },
            "updated": {
                "type": "date"
            },
            "vendor_id": {
                "type": "keyword"
            },
            "product_id": {
                "type": "keyword"
            },
            "location_id": {
                "type": "keyword"
            },
            "stock": {
                "type": "double"
            },
            "price": {
                "type": "double"
            },
            "unit": {
                "type": "boolean"
            },
            "start_time": {
                "type": "date"
            },
            "end_time": {
                "type": "date"
            },
            "vendor": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "keyword"
                    },
                    "created": {
                        "type": "date"
                    },
                    "updated": {
                        "type": "date"
                    },
                    "name": {
                        "type": "text"
                    },
                    "descrip": {
                        "type": "text"
                    }
                }
            },
            "product": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "keyword"
                    },
                    "created": {
                        "type": "date"
                    },
                    "updated": {
                        "type": "date"
                    },
                    "vendor_id": {
                        "type": "text"
                    },
                    "name": {
                        "type": "text"
                    },
                    "descrip": {
                        "type": "text"
                    },
                    "pic_url": {
                        "type": "keyword",
                        "index": false
                    },
                    "attr": {
                        "type": "object"
                    },
                    "attr_num": {
                        "type": "object"
                    }
                }
            },
            "location": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "keyword"
                    },
                    "created": {
                        "type": "date"
                    },
                    "updated": {
                        "type": "date"
                    },
                    "vendor_id": {
                        "type": "text"
                    },
                    "name": {
                        "type": "text"
                    },
                    "descrip": {
                        "type": "text"
                    },
                    "position": {
                        "type": "geo_point"
                    }
                }
            },
            "name": {
                "type": "text"
            },
            "descrip": {
                "type": "text"
            },
            "pic_url": {
                "type": "keyword",
                "index": false
            },
            "attr": {
                "type": "object"
            },
            "attr_num": {
                "type": "object"
            },
            "position": {
                "type": "geo_point"
            }
        }
    }
}