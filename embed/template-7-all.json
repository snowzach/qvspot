{
    "settings": {
        "index": {
            "number_of_replicas": 0,
            "number_of_shards": 3,
            "refresh_interval": "15s",
            "mapping.ignore_malformed": true
        }
    },
    "mappings": {
        "dynamic_templates": [
            {
                "attr": {
                    "path_match": "attr.*",
                    "mapping": {
                        "norms": false,
                        "doc_values": true,
                        "fielddata": false,
                        "type": "keyword"
                    }
                }
            },
            {
                "attr_num": {
                    "path_match": "attr_num.*",
                    "mapping": {
                        "norms": false,
                        "doc_values": true,
                        "fielddata": false,
                        "type": "double"
                    }
                }
            },
            {
                "attr_list": {
                    "path_match": "attr_list.*",
                    "mapping": {
                        "norms": false,
                        "doc_values": true,
                        "fielddata": false,
                        "type": "keyword"
                    }
                }
            },
            {
                "star_as_keyword": {
                    "match_mapping_type": "*",
                    "mapping": {
                        "type": "keyword",
                        "norms": false
                    }
                }
            }
        ],
        "properties": {
            "type": {
                "type": "keyword"
            },
            "id": {
                "type": "keyword"
            },
            "created": {
                "type": "date"
            },
            "updated": {
                "type": "date"
            },
            "name": {
                "type": "text"
            },
            "desc": {
                "type": "text"
            },
            "pic_url": {
                "type": "keyword",
                "index": false
            },
            "product_id": {
                "type": "keyword"
            },
            "vendor_id": {
                "type": "keyword"
            },
            "vendor_location_id": {
                "type": "keyword"
            },
            "location": {
                "type": "geo_point"
            },
            "stock": {
                "type": "double"
            },
            "price": {
                "type": "double"
            },
            "unit": {
                "type": "boolean"
            },
            "start_time": {
                "type": "date"
            },
            "end_time": {
                "type": "date"
            },
            "attr": {
                "type": "object"
            },
            "attr_num": {
                "type": "object"
            },
            "attr_list": {
                "type": "object"
            }
        }
    }
}